<?xml version="1.0" encoding="UTF-8"?>
<natpage natsinglebyte="true" natkcheck="true" xmlns:njx="http://www.softwareag.com/njx/njxMapConverter">
    <titlebar name="Example Library SYSEXV" valueprop="titelText" align="left" image="./images/Natural.png">
    </titlebar>
    <header align="left">
        <button name="Exit" method="onExit" width="80" align="left">
        </button>
        <hdist width="4">
        </hdist>
        <button name="Reset" method="onReset" invisiblemode="disabled" width="80" align="left" valign="middle" title="Reset all fields to initial state">
        </button>
        <hdist width="4">
        </hdist>
        <button name="Execute" method="onRun" image="./images/Run.png" visibleprop="runVisible" invisiblemode="disabled" width="100" align="left" valign="top" titleprop="runTooltip">
        </button>
    </header>
    <pagebody takefullheight="true">
        <rowarea>
            <itr height="30">
                <combodyn2 valueprop="comboVal" validvaluesprop="comboType" width="100" flush="server" flushmethod="onComboSwitch" titleprop="comboToolTip">
                </combodyn2>
                <hdist width="14">
                </hdist>
                <icon image="./images/Search.png" valign="middle" title="Search in member names, descriptions, categories and versions">
                </icon>
                <hdist width="2">
                </hdist>
                <field valueprop="searchText" width="240" flush="server" flushmethod="onSearchText" valign="middle" fieldstyle="font-weight: bold; background-color: #FFFFFF;" title="Search in member names, descriptions, categories and versions">
                </field>
            </itr>
        </rowarea>
        <vsplit height="100%" widthleftprop="widthLeft">
            <splitcell takefullheight="true">
                <rowscrollarea height="100%" takefullwidth="true" takefullheight="true">
                    <clienttree treecollectionprop="treeCollection" withplusminus="true" dynamicloading="false" singleselect="true" selectionvisible="true" withtooltip="true">
                    </clienttree>
                    <vdist height="100%">
                    </vdist>
                </rowscrollarea>
            </splitcell>
            <splitcell takefullheight="true">
                <itr takefullwidth="true" height="24" valign="middle" itrstyle="border: 1px solid #00FFFF">
                    <label name="Properties" width="100%" invisiblemode="invisible" asplaintext="true" height="20" align="center" valign="middle" labelstyle="margin-left: 4px;font-weight: bold;background-color: #BBD8FF;">
                    </label>
                </itr>
                <itr tablestyle="border: 2px solid rgb(255, 255, 255)">
                    <label name="Member" width="100" asplaintext="true" valign="middle" labelstyle="margin-left: 4px;font-weight: bold;background-color: #BBD8FF;">
                    </label>
                    <textout valueprop="textMember" width="100%" bgcolorprop="textBgColor" nowrap="false" valign="middle" textoutstyle="margin-left: 4px;">
                    </textout>
                </itr>
                <itr tablestyle="border: 2px solid rgb(255, 255, 255)">
                    <label name="Version" width="100" asplaintext="true" valign="middle" labelstyle="margin-left: 4px;font-weight: bold;background-color: #BBD8FF;">
                    </label>
                    <textout valueprop="textVersion" width="100%" bgcolorprop="textBgColor" nowrap="false" valign="middle" textoutstyle="margin-left: 4px;">
                    </textout>
                </itr>
                <itr tablestyle="border: 2px solid rgb(255, 255, 255)">
                    <label name="Category" width="100" asplaintext="true" valign="middle" labelstyle="margin-left: 4px;font-weight: bold;background-color: #BBD8FF;">
                    </label>
                    <textout valueprop="textCategory" width="100%" bgcolorprop="textBgColor" nowrap="false" valign="middle" textoutstyle="margin-left: 4px;">
                    </textout>
                </itr>
                <itr tablestyle="border: 2px solid rgb(255, 255, 255)">
                    <label name="Description" width="100" asplaintext="true" valign="middle" labelstyle="margin-left: 4px;font-weight: bold;background-color: #BBD8FF;">
                    </label>
                    <textout valueprop="textDescription" width="100%" bgcolorprop="textBgColor" nowrap="false" valign="middle" textoutstyle="margin-left: 4px;">
                    </textout>
                </itr>
                <itr>
                </itr>
                <itr takefullwidth="true" height="24" visibleprop="headVisible" valign="middle" itrstyle="border: 1px solid #00FFFF">
                    <label name="Example Program" nameprop="textHead" width="100%" invisiblemode="invisible" asplaintext="true" height="20" align="center" valign="middle" labelstyle="margin-left: 4px;font-weight: bold;background-color: #BBD8FF;">
                    </label>
                </itr>
                <rowscrollarea height="100%" takefullwidth="true" takefullheight="true">
                    <itr takefullwidth="true" height="100%" visibleprop="longVisible" align="left" valign="top" itrstyle="background-color: RGB(220, 235, 255);">
                        <hdist width="1">
                        </hdist>
                        <ihtml valueprop="textLong" valign="top" ihtmlstyle="background-color: RGB(220, 235, 255); margin-left: 4px; margin-right: 4px; margin-top: 2px; margin-bottom: 2px; font-family: &quot;Courier New&quot;, Courier, Monospace;">
                        </ihtml>
                    </itr>
                </rowscrollarea>
            </splitcell>
        </vsplit>
    </pagebody>
    <statusbar typeprop="statusType" shorttextprop="statusShort" longtextprop="statusLong">
    </statusbar>
</natpage>
