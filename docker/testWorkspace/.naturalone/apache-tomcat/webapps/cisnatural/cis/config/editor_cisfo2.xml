  <!--
  *****************************************************************************
  * DATATYPEs
  *****************************************************************************
  -->

  <!-- BARCODE : BARCODETYPE -->
  <datatype name="barcodetype">
    <value id="INTERLEAVED2OF5" name="Interleaved 2 of 5"/>
    <value id="CODE39" name="Code 39"/>
    <value id="USD3" name="USD-3"/>
    <value id="3OF9" name="3 of 9"/>
    <value id="CODABAR" name="Codabar"/>
    <value id="USD4" name="USD-4"/>
    <value id="NW7" name="NW-7"/>
    <value id="2OF7" name="2 of 7"/>
    <value id="CODE128" name="Code 128"/>
    <value id="EAN_UCC128" name="EAN/UCC 128"/>
    <value id="UPCA" name="UPC-A"/>
    <value id="UPCE" name="UPC-E"/>
    <value id="EAN13" name="EAN-13"/>
    <value id="EAN8" name="EAN-8"/>
    <value id="POSTNET" name="POSTNET"/>
  </datatype>

  <!-- BARCODE : BASELINE -->
  <datatype name="baselinetype">
    <value id="" name="(default)"/>
    <value id="BOTTOM" name="At bottom"/>
    <value id="TOP" name="At Top"/>
  </datatype>

  <!-- BARCODE : CHECKSUM -->
  <datatype name="checksumtype">
    <value id="" name="(default)"/>
    <value id="ADD" name="Add checksum"/>
    <value id="AUTO" name="Automatic"/>
    <value id="CHECK" name="Check by creation"/>
    <value id="IGNORE" name="Ignore"/>
  </datatype>

  <!-- BARCODE : HUMAN READABLE -->
  <datatype name="humanreadabletype">
    <value id="" name="(default)"/>
    <value id="BOTTOM" name="At bottom"/>
    <value id="NONE" name="None"/>
    <value id="TOP" name="At Top"/>
  </datatype>

  <!-- FOPBORDERSTYLE -->
  <datatype name="fopborderstyle">
    <value id="" name="(default)"/>
    <value id="solid" name="Single line segment."/>
    <value id="none" name="No border"/>
  <!-- not supported by Apache  
    <value id="dashed" name="Series of short line segments"/>
    <value id="dotted" name="Series of dots"/>
    <value id="double" name="Two solid lines"/>
    <value id="groove" name="Looks as though it were carved into the canvas"/>
    <value id="inset" name="The entire box look as though it were embedded in the canvas"/>
    <value id="outset" name="The entire box look as though it were coming out of the canvas"/>
    <value id="ridge" name="Looks as though it were coming out of the canvas"/>
  -->
  </datatype>
  
  <!-- FOPCOLOR -->
  <datatype name="fopcolor">
    <value id="" name="(default)"/>
    <value id="#000000" name="Black"/>
    <value id="#808080" name="Gray"/>
    <value id="#FFFFFF" name="White"/>
    <value id="#FF0000" name="Red"/>
  </datatype>

  <!-- FOPCOLUMCOUNT -->
  <datatype name="fopcolumncount">
    <value id="" name="(default)"/>
    <value id="1" name="1 column"/>
    <value id="2" name="2 Columns"/>
    <value id="3" name="3 Columns"/>
  </datatype>

  <!-- FOPCOLUMWIDTHS -->
  <datatype name="fopcolumnwidths">
    <value id="" name="(default)"/>
    <value id="10cm" name="1 column"/>
    <value id="8cm;8cm" name="2 Columns"/>
    <value id="4cm;5cm;4cm;5cm" name="4 columns"/>
  </datatype>

  <!-- FOPDATATYPE -->
  <datatype name="fopdatatype">
    <value id="" name="(default)"/>
    <value id="date" name="Date (YYYYMMDD)"/>
    <value id="float" name="Float"/>
    <value id="int" name="Integer"/>
    <value id="boolean" name="Boolean"/>
    <value id="time" name="Day time (HHMMSS)"/>
