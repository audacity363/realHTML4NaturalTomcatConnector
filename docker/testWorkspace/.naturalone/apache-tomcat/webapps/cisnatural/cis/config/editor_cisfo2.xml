    <value id="11in" name="11 in."/>
    <value id="indefinite" name="'Page-width' and 'page-height' may not both be set to 'indefinite'."/>
  </datatype>

  <!-- FOPSPANNING -->
  <datatype name="fopspanning">
    <value id="" name="(default)"/>
    <value id="2" name="2 columns or rows."/>
    <value id="3" name="3 columns or rows."/>
  </datatype>

  <!-- FOPTEXTALIGN -->
  <datatype name="foptextalign">
    <value id="" name="(default)"/>
    <value id="left" name="Aligned on the start-edge in the inline-progression-direction."/>
    <value id="center" name="Centered in the inline-progression-direction."/>
    <value id="right" name="Aligned on the end-edge in the inline-progression-direction."/>
  </datatype>

  <!-- FOPTEXTDECORATION -->
  <datatype name="foptextdecoration">
    <value id="" name="(default)"/>
    <value id="overline" name="Each line of text has a line above it."/>
    <value id="line-through" name="Each line of text has a line through the middle."/>
