  </datatype>

  <!-- FOPFONTSIZE -->
  <datatype name="fopfontsize">
    <value id="" name="(default)"/>
    <value id="8pt" name="8 pt"/>
    <value id="10pt" name="10 pt"/>
    <value id="16pt" name="16 pt"/>
  </datatype>

  <!-- FOPFONTSTYLE -->
  <datatype name="fopfontstyle">
    <value id="" name="(default)"/>
    <value id="normal" name="Normal"/>
    <value id="italic" name="Italic style. If not possible, then normal."/>
    <value id="oblique" name="Oblique style. If not possible, then italic."/>
  </datatype>

  <!-- FOPFONTVARIANT -->
  <datatype name="fopfontvariant">
    <value id="" name="(default)"/>
    <value id="normal" name="Normal"/>
    <value id="small-caps" name="Specifies a font that is labeled as a small-caps font."/>
  </datatype>

  <!-- FOPFONTWEIGHT -->
  <datatype name="fopfontweight">
    <value id="" name="(default)"/>
    <value id="normal" name="Normal"/>
    <value id="bold" name="Bold, same as 700"/>
    <value id="100" name="100"/>
    <value id="200" name="200"/>
    <value id="300" name="300"/>
    <value id="400" name="400"/>
    <value id="500" name="500"/>
    <value id="600" name="600"/>
    <value id="700" name="700"/>
    <value id="800" name="800"/>
    <value id="900" name="900"/>
  </datatype>

  <!-- FOPHEIGHT -->
  <datatype name="fopheight">
    <value id="" name="(default)"/>
    <value id="1pt" name="1 pt"/>
    <value id="12pt" name="12 pt"/>
    <value id="0.5in" name="0.5 in"/>
    <value id="1in" name="1 in"/>
    <value id="1mm" name="1 mm"/>
    <value id="5mm" name="5 mm"/>
    <value id="1cm" name="1 cm"/>
    <value id="1.5cm" name="1.5 cm"/>
  </datatype>

  <!-- FOPPAGEDIMENSION -->
  <datatype name="foppagedimension">
    <value id="" name="(default)"/>
    <value id="21cm" name="German DIN A4."/>
    <value id="29.7cm" name="German DIN A4."/>
