<?xml version="1.0" encoding="UTF-8"?>
<wizardconfig>
	<!-- RCHART -->
	<tag name="rchart" wizardclass="com.softwareag.cis.popups.PopupWizardModel"
		wizardpage="popupwizardtag.html">
		<range name="Basic" descriptionid="_desc_basic">
			<attribute name="rcharttype" descriptionid="chart_attr_rcharttype" />
			<attribute name="rcharttypeprop" descriptionid="chart_attr_rcharttypeprop" />
			<attribute name="labels" descriptionid="chart_attr_labels" />
			<attribute name="labelstextid" descriptionid="_attr_textid" />
			<attribute name="labelsprop" descriptionid="chart_attr_labelsprop" />
			<attribute name="valueprop" descriptionid="_attr_valueprop" modus="obligatory" />
			<attribute name="style" descriptionid="_attr_style" />
			<attribute name="styleclasses" descriptionid="bmobile_attr_styleclasses" />
			<attribute name="stackedxaxis" descriptionid="chart_attr_stackedxaxis" />
			<attribute name="stackedyaxis" descriptionid="chart_attr_stackedyaxis" />
			<attribute name="selectedlabelprop" descriptionid="chart_attr_selectedlabelprop" />
			<attribute name="changeindexprop" descriptionid="_attr_changeindexprop" />
			<attribute name="showvalueinchart" descriptionid="_attr_showvalueinchart" />
			<attribute name="showvaluepos" descriptionid="_attr_showvaluepos" />
			<attribute name="showtooltip" descriptionid="_attr_showtooltip" />
			<attribute name="showvaluefontfamily" descriptionid="_attr_fontfamily" />
			<attribute name="showvaluefontstyle" descriptionid="_attr_fontstyle" />
			<attribute name="showvaluefontsize" descriptionid="_attr_fontsize" />
			<attribute name="showvaluefontcolor" datatype="_attr_color" />
			<attribute name="comment" descriptionid="_attr_comment" />
		</range>
		<range name="BarOptions" descriptionid="_desc_baroptions">
			<attribute name="xaxisscalelabel" descriptionid="chart_attr_scalelabel" />
			<attribute name="xaxisscaletextid" descriptionid="_attr_textid" />
			<attribute name="yaxisscalelabel" descriptionid="chart_attr_scalelabel" />
			<attribute name="yaxisscaletextid" descriptionid="_attr_textid" />
			<attribute name="xaxisbarthickness" descriptionid="chart_attr_xaxisbarthickness" />
			<attribute name="xaxisbarpercentage" descriptionid="chart_attr_xaxisbarpercentage" />
			<attribute name="xaxiscategorypercentage" descriptionid="chart_attr_xaxiscategorypercentage" />
			<attribute name="yaxisbarthickness" descriptionid="chart_attr_yaxisbarthickness" />			
		</range>
		<range name="LineOptions" descriptionid="_desc_lineoptions">
			<attribute name="showlines" descriptionid="chart_attr_showlines" />
		</range>
		<range name="Miscellaneous" descriptionid="_desc_Miscellaneous">
			<attribute name="testtoolid" descriptionid="_attr_testtoolid" />
		</range>
	</tag>

	<!-- RCHARTDATA -->
	<tag name="rchartdata" wizardclass="com.softwareag.cis.popups.PopupWizardModel"
		wizardpage="popupwizardtag.html">
		<range name="Basic" descriptionid="_desc_basic">
			<attribute name="valueprop" descriptionid="_attr_valueprop" modus="obligatory"/>
			<attribute name="comment" descriptionid="_attr_comment" />
			<attribute name="label" descriptionid="chart_attr_label" />
			<attribute name="labeltextid" descriptionid="_attr_textid" />
			<attribute name="datatype" descriptionid="_attr_datatype" />
			<attribute name="backgroundcolor" descriptionid="chart_attr_backgroundcolor" />
			<attribute name="borderwidth" descriptionid="chart_attr_borderwidth" />
			<attribute name="bordercolor" descriptionid="chart_attr_bordercolor" />
			<attribute name="onclickmethod" descriptionid="chart_attr_onclickmethod" />
			<attribute name="legendclickmethod" descriptionid="chart_attr_legendonclickmethod" />
		</range>
		<range name="Bar" descriptionid="_desc_barchart">
			<attribute name="hoverbackgroundcolor" descriptionid="chart_attr_hoverbackgroundcolor" />
			<attribute name="hoverborderwidth" descriptionid="chart_attr_hoverborderwidth" />
			<attribute name="hoverbordercolor" descriptionid="chart_attr_hoverbordercolor" />
		</range>
		<range name="Line" descriptionid="_desc_linechart">
			<attribute name="borderdash" descriptionid="chart_attr_borderdash" />
			<attribute name="pointbordercolor" descriptionid="chart_attr_pointbordercolor" />
			<attribute name="pointbackgroundcolor" descriptionid="chart_attr_pointbackgroundcolor" />
			<attribute name="pointborderwidth" descriptionid="chart_attr_pointborderwidth" />
			<attribute name="pointradius" descriptionid="chart_attr_pointradius" />
			<attribute name="pointhoverradius" descriptionid="chart_attr_pointhoverradius" />
			<attribute name="pointhitradius" descriptionid="chart_attr_pointhitradius" />
			<attribute name="pointhoverbackgroundcolor" descriptionid="chart_attr_pointhoverbackgroundcolor" />
			<attribute name="pointhoverbordercolor" descriptionid="chart_attr_pointhoverbordercolor" />
			<attribute name="pointhoverborderwidth" descriptionid="chart_attr_pointhoverborderwidth" />
			<attribute name="pointstyle" descriptionid="chart_attr_pointstyle" />
			<attribute name="steppedline" descriptionid="chart_attr_steppedline" />
		</range>
		<range name="Binding Bar" descriptionid="_desc_binding">
		    <attribute name="labelprop" descriptionid="chart_attr_labelprop" />
		    <attribute name="backgroundcolorprop" descriptionid="chart_attr_backgroundcolorprop" />
			<attribute name="borderwidthprop" descriptionid="chart_attr_borderwidthprop" />
			<attribute name="bordercolorprop" descriptionid="chart_attr_bordercolorprop" />
			<attribute name="hoverbackgroundcolorprop" descriptionid="chart_attr_hoverbackgroundcolorprop" />
			<attribute name="hoverborderwidthprop" descriptionid="chart_attr_hoverborderwidthprop" />
			<attribute name="hoverbordercolorprop" descriptionid="chart_attr_hoverbordercolorprop" />
		</range>
		<range name="Binding Line" descriptionid="_desc_binding">
		    <attribute name="labelprop" descriptionid="chart_attr_labelprop" />
		    <attribute name="backgroundcolorprop" descriptionid="chart_attr_backgroundcolorprop" />
			<attribute name="borderwidthprop" descriptionid="chart_attr_borderwidthprop" />
			<attribute name="bordercolorprop" descriptionid="chart_attr_bordercolorprop" />
			<attribute name="borderdashprop" descriptionid="chart_attr_borderdashprop" />
			<attribute name="pointbordercolorprop" descriptionid="chart_attr_pointbordercolorprop" />
			<attribute name="pointbackgroundcolorprop" descriptionid="chart_attr_pointbackgroundcolorprop" />
			<attribute name="pointborderwidthprop" descriptionid="chart_attr_pointborderwidthprop" />
			<attribute name="pointradiusprop" descriptionid="chart_attr_pointradiusprop" />
			<attribute name="pointhoverradiusprop" descriptionid="chart_attr_pointhoverradiusprop" />
			<attribute name="pointhitradiusprop" descriptionid="chart_attr_pointhitradiusprop" />
			<attribute name="pointhoverbackgroundcolorprop" descriptionid="chart_attr_pointhoverbackgroundcolorprop" />
			<attribute name="pointhoverbordercolorprop" descriptionid="chart_attr_pointhoverbordercolorprop" />
			<attribute name="pointhoverborderwidthprop" descriptionid="chart_attr_pointhoverborderwidthprop" />
			<attribute name="pointstyleprop" descriptionid="chart_attr_pointstyleprop" />
			<attribute name="steppedlineprop" descriptionid="chart_attr_steppedlineprop" />
		</range>
	</tag>

	<!-- RPIECHART -->
	<tag name="rpiechart" wizardclass="com.softwareag.cis.popups.PopupWizardModel"
