<?xml version="1.0" encoding="UTF-8"?>

<!--
Dynamic extension of editor.xml file.
-->

<controllibrary>
  <editor>

  <!--
  *****************************************************************************
  * DATATYPEs
  *****************************************************************************
  -->


  <!--
  *****************************************************************************
  * TAGs
  *****************************************************************************
  -->

  <!-- DROPICON -->
  <tag name="dropicon">
    <attribute name="image" datatype="imagefilename"/>
    <attribute name="imageprop" datatype="property"/>
    <attribute name="title"/>
    <attribute name="titletextid" datatype="textid"/>
    <attribute name="method" datatype="method"/>
    <attribute name="draginfo"/>
    <attribute name="draginfoprop" datatype="property"/>
    <attribute name="dropmethod" datatype="method"/>
    <attribute name="dropinfoprop" datatype="property"/>
    <attribute name="visibleprop" datatype="property"/>
    <attribute name="invisiblemode" datatype="invisiblemode"/>
    <attribute name="imageinactive"/>
    <attribute name="align" datatype="align"/>
    <attribute name="valign" datatype="valign"/>
    <attribute name="withdistance" datatype="boolean"/>
    <attribute name="imagewidth"/>
    <attribute name="imageheight"/>
    <attribute name="colstyle" datatype="style"/>
    <attribute name="spanstyle" datatype="style"/>
    <attribute name="tabindex" datatype="tabindex"/>
    <attribute name="comment" inputtype="multiline"/>
    <attribute name="titleprop" datatype="property"/>
    <taginstance>
        <icon image="$image$" imageprop="$imageprop$" title="$title$" titletextid="$titletextid$" titleprop="$titleprop$" method="$method$" dropmethod="$dropmethod$" draginfo="$draginfo$" draginfoprop="$draginfoprop$" dropinfoprop="$dropinfoprop$" visibleprop="$visibleprop$" invisiblemode="$invisiblemode$" imageinactive="$imageinactive$" align="$align$" valign="$valign$" withdistance="$withdistance$" imagewidth="$imagewidth$" imageheight="$imageheight$" colstyle="$colstyle$" spanstyle="$spanstyle$" tabindex="$tabindex$" comment="$comment$">
        </icon>
    </taginstance>
    <protocolitem>
        <addproperty name="$draginfoprop$" datatype="String" useincodegenerator="true"/>
        <addproperty name="$visibleprop$" datatype="boolean" useincodegenerator="true"/>
        <addproperty name="$dropinfoprop$" datatype="String" useincodegenerator="true"/>
        <addproperty name="$titleprop$" datatype="String" useincodegenerator="true"/>
        <addproperty name="$imageprop$" datatype="String" useincodegenerator="true"/>
        <addmethod name="$method$" useincodegenerator="true"/>
        <addmethod name="$dropmethod$" useincodegenerator="true"/>
    </protocolitem>
  </tag>
  <tagsubnodeextension control="cellspan" newsubnode="dropicon"/>
  <tagsubnodeextension control="header" newsubnode="dropicon"/>
  <tagsubnodeextension control="itr" newsubnode="dropicon"/>
  <tagsubnodeextension control="str" newsubnode="dropicon"/>
  <tagsubnodeextension control="titlebar" newsubnode="dropicon"/>
  <tagsubnodeextension control="tr" newsubnode="dropicon"/>
  <tagsubnodeextension control="treenode3" newsubnode="dropicon"/>


  </editor>
</controllibrary>