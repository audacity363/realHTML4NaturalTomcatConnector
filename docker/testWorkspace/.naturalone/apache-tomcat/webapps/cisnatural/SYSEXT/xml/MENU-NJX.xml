<?xml version="1.0" encoding="UTF-8"?>
<natpage natsinglebyte="true" natkcheck="true" xmlns:njx="http://www.softwareag.com/njx/njxMapConverter">
    <titlebar name="SYSEXT - Application Programming Interfaces" valueprop="titelText" align="left" closetitle="Exit SYSEXT" image="./images/Natural.png">
    </titlebar>
    <header align="left" nocellspacing="true">
        <button name="Exit" method="onExit" width="80" align="left" valign="middle" title="Exit SYSEXT">
        </button>
        <hdist width="4">
        </hdist>
        <button name="Refresh" method="onRefresh" invisiblemode="disabled" width="80" align="left" valign="middle" title="Update the API information. Only required if API descriptions have been changed.">
        </button>
        <hdist width="4">
        </hdist>
        <button name="Open All" method="onOpen" visibleprop="openVisible" invisiblemode="disabled" width="80" align="left" valign="middle" title="Open all nodes in the tree">
        </button>
        <hdist width="4">
        </hdist>
        <button name="Close All" method="onClose" visibleprop="closeVisible" invisiblemode="disabled" width="80" align="left" valign="middle" title="Close all nodes in the tree">
        </button>
        <hdist width="4">
        </hdist>
        <button name="Reset" method="onReset" invisiblemode="disabled" width="80" align="left" valign="middle" title="Reset all fields to initial state">
        </button>
        <hdist width="4">
        </hdist>
        <button name="Execute" method="onRun" image="./images/Run.png" visibleprop="runVisible" invisiblemode="disabled" width="100" align="left" valign="middle" titleprop="runTooltip">
        </button>
        <hdist width="4">
        </hdist>
    </header>
    <pagebody takefullheight="true">
        <rowarea>
            <itr height="30" valign="middle" itrstyle="background-color: RGB(220, 235, 255);">
                <combodyn2 valueprop="comboVal" validvaluesprop="comboType" width="160" flush="server" flushmethod="onComboSwitch" combostyle="font-weight: bold;background-color: #BBD8FF;" titleprop="comboToolTip">
                </combodyn2>
                <hdist width="4">
                </hdist>
                <combodyn2 valueprop="comboDepVal" validvaluesprop="comboDep" width="160" flush="server" flushmethod="onComboDepSwitch" combostyle="font-weight: bold;background-color: #BBD8FF;" titleprop="comboDepToolTip">
                </combodyn2>
                <hdist width="14">
                </hdist>
                <icon image="./images/Search.png" valign="middle" title="Search in API names, purposes, categories and keywords">
                </icon>
                <hdist width="2">
                </hdist>
                <field valueprop="searchText" width="240" flush="server" flushmethod="onSearchText" valign="middle" fieldstyle="font-weight: bold; background-color: #FFFFFF;" title="Search in API names, purposes, categories and keywords">
                </field>
            </itr>
        </rowarea>
        <vsplit height="100%" widthleftprop="widthLeft">
            <splitcell takefullheight="true">
                <rowscrollarea height="100%" takefullwidth="true" takefullheight="true">
                    <clienttree treecollectionprop="treeCollection" withplusminus="true" dynamicloading="false" singleselect="true" selectionvisible="true" withtooltip="true">
                    </clienttree>
                    <vdist height="100%">
                    </vdist>
                </rowscrollarea>
            </splitcell>
            <splitcell takefullheight="true">
                <itr>
                    <hdist width="1">
                    </hdist>
                    <label name="Purpose" width="100" asplaintext="true" valign="middle" labelstyle="margin-left: 4px;font-weight: bold;background-color: #BBD8FF;">
                    </label>
                    <textout valueprop="textDescription" width="100%" bgcolorprop="textBgColor" nowrap="false" valign="middle" textoutstyle="margin-left: 4px; margin-top: 4px;">
                    </textout>
                </itr>
                <itr>
                    <hdist width="1">
                    </hdist>
                    <label name="API Name" width="100" asplaintext="true" valign="middle" labelstyle="margin-left: 4px;font-weight: bold;background-color: #BBD8FF;">
                    </label>
                    <textout valueprop="textUserexit" width="100%" bgcolorprop="textBgColor" nowrap="false" valign="middle" textoutstyle="margin-left: 4px;">
                    </textout>
                </itr>
                <itr>
                    <hdist width="1">
                    </hdist>
                    <label name="Product" width="100" asplaintext="true" valign="middle" labelstyle="margin-left: 4px;font-weight: bold;background-color: #BBD8FF;">
                    </label>
                    <textout valueprop="textProduct" width="100%" bgcolorprop="textBgColor" nowrap="false" valign="middle" textoutstyle="margin-left: 4px;">
                    </textout>
                </itr>
                <itr>
                    <hdist width="1">
                    </hdist>
                    <label name="Category" width="100" asplaintext="true" valign="middle" labelstyle="margin-left: 4px;font-weight: bold;background-color: #BBD8FF;">
                    </label>
                    <textout valueprop="textCategory" width="100%" bgcolorprop="textBgColor" nowrap="false" valign="middle" textoutstyle="margin-left: 4px;">
                    </textout>
                </itr>
                <itr>
                    <hdist width="1">
                    </hdist>
                    <label name="Keywords" width="100" asplaintext="true" valign="middle" labelstyle="margin-left: 4px;font-weight: bold;background-color: #BBD8FF;">
                    </label>
                    <textout valueprop="textKeyword" width="100%" bgcolorprop="textBgColor" nowrap="false" valign="middle" textoutstyle="margin-left: 4px;">
                    </textout>
                </itr>
                <itr>
                </itr>
                <itr takefullwidth="true" height="36" valign="middle">
                    <hdist width="1">
                    </hdist>
                    <combodyn2 valueprop="listVal" validvaluesprop="listType" width="100%" flush="server" flushmethod="onListSwitch" statusprop="listStatus" combostyle="font-weight: bold;background-color: #BBD8FF;" titleprop="listToolTip">
                    </combodyn2>
                </itr>
                <rowscrollarea height="100%" takefullwidth="true" takefullheight="true">
                    <itr takefullwidth="true" height="100%" visibleprop="longVisible" align="left" valign="top" itrstyle="background-color: RGB(220, 235, 255);">
                        <hdist width="1">
                        </hdist>
                        <ihtml valueprop="textLong" valign="top" ihtmlstyle="background-color: RGB(220, 235, 255); margin-left: 4px; margin-right: 4px; margin-top: 2px; margin-bottom: 2px; font-family: &quot;Courier New&quot;, Courier, Monospace;">
                        </ihtml>
                    </itr>
                    <vdist height="100%">
                    </vdist>
                </rowscrollarea>
            </splitcell>
        </vsplit>
    </pagebody>
    <statusbar typeprop="statusType" shorttextprop="statusShort" longtextprop="statusLong">
    </statusbar>
</natpage>
