<?xml version="1.0" encoding="UTF-8"?>
<natpage hotkeys="13;onLogon">
    <titlebar name="Disconnected From Natural" withclose="false">
    </titlebar>
    <xciwpaccess>
    </xciwpaccess>
    <pagebody takefullheight="false">
        <vdist height="5px">
        </vdist>
        <itr align="left">
            <hdist width="40">
            </hdist>
            <icon image="../cisnatural/images/SAG_logo_login_200x73.png" align="left">
            </icon>
        </itr>
        <vdist height="10">
        </vdist>
        <hline height="10" color="#00A1E1">
        </hline>
        <vdist height="15">
        </vdist>
        <itr align="right">
            <textout valueprop="versionInfo" width="100%" textsize="2" textcolor="#233356" align="right" textoutstyle="font-weight: bold">
            </textout>
            <hdist width="10">
            </hdist>
        </itr>
        <rowarea name="Disconnect Message" foldable="false">
            <itr takefullwidth="true">
                <textout valueprop="disconnectMessage" width="80%" fgcolorprop="colorProp">
                </textout>
            </itr>
            <vdist height="15">
            </vdist>
            <itr takefullwidth="true">
                <button method="onLogon" nameprop="buttonName" width="200">
                </button>
            </itr>
        </rowarea>
        <vdist>
        </vdist>
    </pagebody>
    <statusbar withdistance="false">
    </statusbar>
</natpage>
