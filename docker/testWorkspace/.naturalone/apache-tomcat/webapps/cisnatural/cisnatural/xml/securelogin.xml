<?xml version="1.0" encoding="UTF-8"?>
<natpage hotkeys="13;doLogin" natsinglebyte="true" xmlns:njx="http://www.softwareag.com/njx/njxMapConverter">
    <titlebar name="Logon to Natural for Ajax" withclose="false">
    </titlebar>
    <pagebody>
        <vdist height="5px">
        </vdist>
        <itr align="left">
            <hdist width="40">
            </hdist>
            <icon image="../cisnatural/images/SAG_logo_login_200x73.png" align="left">
            </icon>
        </itr>
        <vdist height="10">
        </vdist>
        <hline height="10" color="#00A1E1">
        </hline>
        <vdist height="15">
        </vdist>
        <itr align="right">
            <textout valueprop="versionInfo" width="100%" textsize="2" textcolor="#233356" align="right" textoutstyle="font-weight: bold">
            </textout>
            <hdist width="10">
            </hdist>
        </itr>
        <rowarea name="Logon credentials" visibleprop="credsRowVisible">
            <itr>
                <label name="User name:" width="130">
                </label>
                <hdist width="10">
                </hdist>
                <field valueprop="userNameProp">
                </field>
            </itr>
            <vdist height="10">
            </vdist>
            <itr>
                <label name="Password:" width="130">
                </label>
                <hdist width="10">
                </hdist>
                <field valueprop="passwordProp" password="true">
                </field>
                <hdist width="50">
                </hdist>
                <button name="Logon" method="doLogin">
                </button>
            </itr>
            <vdist height="10">
            </vdist>
        </rowarea>
        <rowarea name="Error output" visibleprop="errorRowVisible">
            <itr takefullwidth="true">
                <textout valueprop="errorText" width="100%" textcolor="#FF0000">
                </textout>
            </itr>
        </rowarea>
    </pagebody>
    <statusbar withdistance="false">
    </statusbar>
    <itr>
    </itr>
</natpage>
