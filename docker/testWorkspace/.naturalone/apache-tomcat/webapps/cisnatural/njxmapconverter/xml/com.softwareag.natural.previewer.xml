<?xml version="1.0" encoding="UTF-8"?>
<page model="com.softwareag.natural.mapconverter.gui.PreviewerAdapter">
    <titlebar name="Page Previewer">
    </titlebar>
    <header align="left" withdistance="false">
    </header>
    <pagebody horizdist="false" takefullheight="true" paddingleft="20" paddingright="20" paddingtop="20" paddingbottom="20">
        <rowareawithheader visibleprop="selectLayout">
            <rowareaheader name="Select Layout">
                <icon image="../HTMLBasedGUI/images/preview2.gif" method="onPreviewLayout" valign="middle" title="Preview Layout XML Definition">
                </icon>
            </rowareaheader>
            <rowareabody>
                <vdist height="10">
                </vdist>
                <itr>
                    <label name="Project" width="100" title="Select the project containing the layout.">
                    </label>
                    <combodyn2 valueprop="projectName" validvaluesprop="projectValues" width="250" flush="server" flushmethod="onProjectChanged">
                    </combodyn2>
                </itr>
                <vdist height="10">
                </vdist>
                <itr>
                    <label name="Layout" width="100" title="Select the layout to preview.">
                    </label>
                    <combodyn2 valueprop="layoutName" validvaluesprop="layoutNameValues" width="250" flush="server" flushmethod="onLayoutChanged">
                    </combodyn2>
                </itr>
                <vdist height="10">
                </vdist>
            </rowareabody>
        </rowareawithheader>
        <rowarea name="Preview" height="100%">
            <tr height="100%">
                <subcispage2 subcispageprop="previewPage" width="100%" height="100%" borderwidth="1" withownborder="true">
                </subcispage2>
            </tr>
        </rowarea>
    </pagebody>
    <statusbar withdistance="false">
    </statusbar>
</page>
