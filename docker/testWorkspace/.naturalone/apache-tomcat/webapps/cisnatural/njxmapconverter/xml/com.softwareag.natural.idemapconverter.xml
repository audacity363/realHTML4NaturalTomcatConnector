<?xml version="1.0" encoding="UTF-8"?>
<page model="com.softwareag.natural.mapconverter.gui.IDEMapConverterAdapter">
    <titlebar name="IDE Map Converter">
    </titlebar>
    <header withdistance="false">
        <button name="Execute Event" method="onExecuteEvent">
        </button>
    </header>
    <pagebody>
        <rowarea name="Event Data">
            <itr>
                <label name="Event name" width="150">
                </label>
                <field valueprop="eventname" width="200">
                </field>
            </itr>
            <vdist height="10">
            </vdist>
            <itr>
                <label name="Map extract folder" width="150">
                </label>
                <field valueprop="mapextractfolder" width="200">
                </field>
            </itr>
            <vdist height="10">
            </vdist>
            <itr>
                <label name="Map extract name" width="150">
                </label>
                <field valueprop="mapextractname" width="200">
                </field>
            </itr>
            <vdist height="10">
            </vdist>            
            <itr>
                <label name="Rules name" width="150">
                </label>
                <field valueprop="rulesname" width="200">
                </field>
            </itr>
            <vdist height="10">
            </vdist>
            <itr>
                <label name="Rules project" width="150">
                </label>
                <field valueprop="rulesproject" width="200">
                </field>
            </itr>            
            <vdist height="10">
            </vdist>
            <itr>
                <label name="Project name" width="150">
                </label>
                <field valueprop="projectname" width="200">
                </field>
            </itr>
            <vdist height="10">
            </vdist>
            <itr>
                <label name="Log folder" width="150">
                </label>
                <field valueprop="logfolder" width="200">
                </field>
            </itr>
            <vdist height="10">
            </vdist>
            <itr>
                <label name="Log name" width="150">
                </label>
                <field valueprop="logname" width="200">
                </field>
            </itr>
            <vdist height="10">
            </vdist>
            <itr>
                <text valueprop="inputdata" width="350" height="200">
                </text>
            </itr>
        </rowarea>
    </pagebody>
    <statusbar withdistance="false">
    </statusbar>
</page>
