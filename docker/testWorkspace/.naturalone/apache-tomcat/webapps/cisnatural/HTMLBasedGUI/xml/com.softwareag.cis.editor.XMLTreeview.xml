<?xml version="1.0" encoding="UTF-8"?>
<page model="com.softwareag.cis.editor.XMLTreeviewAdapter" translationreference="editor">
    <titlebar textid="rexmltv.titlebar1" visibleprop="showTitlebar" closetitletextid="closePage">
    </titlebar>
    <pagebody horizdist="false" takefullheight="true">
        <vsplit height="100%" widthleftprop="widhtLeft">
            <splitcell takefullheight="true">
                <itr visibleprop="showLevelSelection">
                    <hdist>
                    </hdist>
                    <icon image="../HTMLBasedGUI/images/open.gif" method="onCreatePage" titletextid="rexmltv.icon1">
                    </icon>
                    <hdist width="16">
                    </hdist>
                    <label asplaintext="true" textid="rexmltv.label3">
                    </label>
                    <hdist width="4">
                    </hdist>
                    <field datatype="int" flush="server" length="2" valueprop="levelsWithAllContent">
                    </field>
                    <hdist width="100%">
                    </hdist>
                </itr>
                <hline color="#C0C0C0" height="1">
                </hline>
                <rowtablearea2 darkbackground="false" firstrowcolwidths="true" griddataprop="nodes" height="100%" rowcount="38" tablestyle="background-color: #FFFFFF" width="100%" withborder="false">
                    <repeat>
                        <str showifempty="false" valueprop="selected">
                            <treenode3 enabledrag="false" width="100%">
                            </treenode3>
                        </str>
                    </repeat>
                </rowtablearea2>
                <hline color="#C0C0C0" height="1">
                </hline>
            </splitcell>
            <splitcell takefullheight="true">
                <rowarea fixlayout="true" foldable="false" height="100%" image="images/edit.gif" textid="rexmltv.rowarea4" visibleprop="showText" withbottomborder="false" withleftborder="false" withrightborder="false" withtopborder="false" withtoppadding="false">
                    <itr fixlayout="true" itrstyle="background-color: #FFFFFF" takefullwidth="true">
                        <textout textoutstyle="background-color: #FFFFFF" valueprop="textContent" width="100%">
                        </textout>
                    </itr>
                    <vdist backgroundstyle="background-color: #FFFFFF" height="100%">
                    </vdist>
                </rowarea>
                <rowarea fixlayout="true" foldable="false" height="100%" image="images/edit.gif" textid="rexmltv.rowarea5" visibleprop="showTextContent" withleftborder="false" withrightborder="false" withtopborder="false" withtoppadding="false">
                    <rowtablearea2 firstrowcolwidths="true" griddataprop="attributes" height="100%" rowcount="30" width="100%">
                        <tr>
                            <label asheadline="true" textid="rexmltv.label4" width="50%">
                            </label>
                            <label asheadline="true" textid="rexmltv.label5" width="50%">
                            </label>
                        </tr>
                        <repeat>
                            <str showifempty="false" valueprop="selected">
                                <field displayonly="true" noborder="true" valueprop="attribute" width="100%">
                                </field>
                                <field noborder="true" valueprop="attributeValue" width="100%">
                                </field>
                            </str>
                        </repeat>
                    </rowtablearea2>
                </rowarea>
                <rowarea fixlayout="true" foldable="true" height="180" image="images/new.gif" textid="rexmltv.rowarea6" visibleprop="showGrid" withleftborder="false" withrightborder="false">
                    <rowtabsubpages pagesprop="showNodePage" triggerserver="true" height="100%" borderwidth="0">
                    </rowtabsubpages>
                </rowarea>
                <rowdynavis valueprop="showNamespaces">
                    <rowarea image="images/edit.gif" textid="rexmltv.rowarea7" withbottomborder="false" withleftborder="false" withrightborder="false">
                        <itr takefullwidth="true">
                            <textgridsss2 griddataprop="namespaces" hscroll="true" rowcount="5" showemptylines="false" width="100%" withtitlerow="false">
                                <column property="ns" width="150">
                                </column>
                                <column property="value" width="100%">
                                </column>
                            </textgridsss2>
                        </itr>
                    </rowarea>
                </rowdynavis>
            </splitcell>
        </vsplit>
        <hline color="#C0C0C0" height="1">
        </hline>
        <itr visibleprop="showLevelSelection">
            <hdist>
            </hdist>
            <field displayonly="true" fieldstyle="background-color: #FFFFFF" noborder="true" valueprop="path" width="100%">
            </field>
            <hdist>
            </hdist>
        </itr>
    </pagebody>
</page>
