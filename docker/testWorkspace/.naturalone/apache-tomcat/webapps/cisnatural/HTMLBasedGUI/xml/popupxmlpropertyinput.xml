<?xml version="1.0" encoding="UTF-8"?>
<page model="com.softwareag.cis.editor.EditorXMLPropertyInputAdapter6" translationreference="CIS_XMLDATA">
    <header withdistance="false">
        <button textid="xe.btn.saveAndClose" method="onSave" image="../HTMLBasedGUI/images/save.gif" visibleprop="canBeSaved" invisiblemode="disabled" imagedisabled="../HTMLBasedGUI/images/saveinactive.gif">
        </button>
        <button textid="xe.btn.cancel" method="closePage" invisiblemode="disabled">
        </button>
    </header>
    <pagebody takefullheight="true">
        <vdist>
        </vdist>
        <itr takefullwidth="true" height="100%">
            <coltable0 width="100%" takefullheight="true">
                <rowtabsubpages pagesprop="tabPageInfo" triggerserver="true" height="100%" borderwidth="0" pagestyle="border-right:0">
                </rowtabsubpages>
            </coltable0>
            <coltable0 width="350" takefullheight="true">
                <itr takefullwidth="true" height="100%">
                    <subpage valueprop="editorHelpURL" height="100%" width="100%" pagestyle="border:0">
                    </subpage>
                </itr>
            </coltable0>
        </itr>
        <vdist>
        </vdist>
        <rowdynavis valueprop="modeVis" height="100">
            <rowareawithheader withtoppadding="false">
                <rowareaheader textid="xe.addNewMode">
                    <hdist width="100%">
                    </hdist>
                    <icon image="../HTMLBasedGUI/images/close.gif" method="onCancelNewMode" valign="middle">
                    </icon>
                    <hdist width="2">
                    </hdist>
                </rowareaheader>
                <rowareabody>
                    <itr>
                        <label textid="xe.modeName" width="100">
                        </label>
                        <field valueprop="modeName" width="200">
                        </field>
                    </itr>
                    <vdist>
                    </vdist>
                    <itr>
                        <hdist width="100">
                        </hdist>
                        <button textid="xe.btn.createExecute" method="onSaveNewMode">
                        </button>
                    </itr>
                    <vdist>
                    </vdist>
                    <itr>
                        <hdist width="100">
                        </hdist>
                        <label textid="xe.createNewModeHelp" asplaintext="true">
                        </label>
                    </itr>
                    <vdist>
                    </vdist>
                </rowareabody>
            </rowareawithheader>
            <vdist>
            </vdist>
        </rowdynavis>
    </pagebody>
    <statusbar withdistance="false">
    </statusbar>
</page>
