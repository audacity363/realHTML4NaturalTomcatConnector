<?xml version="1.0" encoding="UTF-8"?>
<page model="com.softwareag.cis.editor.AttributeCompareEditor" translationreference="CIS_EDIT">
    <pagebody horizdist="false" takefullheight="true">
        <itr width="100%">
            <label asheadline="true" height="14" labelstyle="padding-left: 2px; font-size: 10px; font-weight: bold" textid="attribComp.label0">
            </label>
        </itr>
        <vdist height="8">
        </vdist>
        <rowtablearea2 griddataprop="tree" height="100%" hscroll="false" rowcount="30" width="100%" withborder="false">
            <repeat>
                <tr>
                    <hdist width="10">
                    </hdist>
                    <treenode3 enabledrag="true" imageclosed="images/xmlclosed.gif" imageendnode="images/xmlendnode.gif" imageopened="images/xmlopened.gif" singleselect="false" width="500" withlines="true" withplusminus="true" withtextinput="false" withtooltip="true">
                        <hdist>
                        </hdist>
                    </treenode3>
                </tr>
            </repeat>
        </rowtablearea2>
        <rowarea withbottomborder="false" withleftborder="false" withrightborder="false" withtopborder="false" withtoppadding="false">
            <itr align="center">
                <icon image="images/arrowupfast.gif" method="onMoveToTop">
                </icon>
                <hdist width="5">
                </hdist>
                <icon image="images/arrowup.gif" method="onMoveUp">
                </icon>
                <hdist width="5">
                </hdist>
                <icon image="images/arrowdown.gif" method="onMoveDown">
                </icon>
                <hdist width="5">
                </hdist>
                <icon image="images/arrowdownfast.gif" method="onMoveToBottom">
                </icon>
                <hdist width="5">
                </hdist>
                <icon image="images/new.gif" method="onNew" titletextid="attribComp.icon0">
                </icon>
                <hdist width="5">
                </hdist>
                <icon image="images/remove.gif" method="onRemove" titletextid="attribComp.icon1">
                </icon>
            </itr>
        </rowarea>
        <itr width="100%">
            <label asheadline="true" height="14" labelstyle="padding-left: 2px; font-size: 10px; font-weight: bold" textid="attribComp.label1">
            </label>
        </itr>
        <rowarea visibleprop="editButtonVis" withbottomborder="false" withleftborder="false" withrightborder="false" withtopborder="false" withtoppadding="false">
            <vdist height="5">
            </vdist>
            <itr>
                <hdist width="10px">
                </hdist>
                <label textid="attribComp.label2" width="120">
                </label>
                <field statusprop="resourceState" valueprop="project" width="100%">
                </field>
                <hdist width="10px">
                </hdist>
            </itr>
            <itr>
                <hdist width="10px">
                </hdist>
                <label textid="attribComp.label3" width="120">
                </label>
                <field statusprop="resourceState" valueprop="mlFile" width="100%">
                </field>
                <hdist width="10px">
                </hdist>
            </itr>
            <vdist height="5">
            </vdist>
        </rowarea>
        <itr width="100%">
            <label asheadline="true" height="14" labelstyle="padding-left: 2px; font-size: 10px; font-weight: bold" textid="attribComp.label4">
            </label>
        </itr>
        <rowarea height="50" withbottomborder="false" withleftborder="false" withrightborder="false" withtopborder="false" withtoppadding="false">
            <vdist height="5">
            </vdist>
            <itr height="100%" width="100%">
                <hdist width="10">
                </hdist>
                <text displayonly="true" height="100%" textareastyle="border:0; overflow-y: hidden" valueprop="attributeInfo" width="100%">
                </text>
            </itr>
            <vdist height="5">
            </vdist>
        </rowarea>
        <itr width="100%">
            <label asheadline="true" height="14" labelstyle="padding-left: 2px; font-size: 10px; font-weight: bold" textid="attribComp.label5">
            </label>
        </itr>
        <rowarea visibleprop="editButtonVis" withbottomborder="false" withleftborder="false" withrightborder="false" withtopborder="false" withtoppadding="false">
            <vdist height="10">
            </vdist>
            <itr>
                <hdist width="10px">
                </hdist>
                <label textid="attribComp.label6" width="120">
                </label>
                <field displayonly="true" valueprop="wizardConfigFileName" width="100%">
                </field>
                <hdist width="10">
                </hdist>
                <button image="../HTMLBasedGUI/images/save.gif" method="onSave" textid="attribComp.button0">
                </button>
                <hdist width="10px">
                </hdist>
            </itr>
            <vdist height="10">
            </vdist>
        </rowarea>
    </pagebody>
    <statusbar>
    </statusbar>
</page>
