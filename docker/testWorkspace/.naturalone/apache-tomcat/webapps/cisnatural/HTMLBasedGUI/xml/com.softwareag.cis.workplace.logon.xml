<?xml version="1.0" encoding="UTF-8"?>
<page model="com.softwareag.cis.zzz.LogonModel" translationreference="ErrorReaction">
    <titlebar textid="logonTitle" closetitletextid="closePage">
    </titlebar>
    <header withdistance="false">
        <button method="logon" textid="logon">
        </button>
    </header>
    <pagebody takefullheight="true">
        <vdist height="50%">
        </vdist>
        <itr width="100%">
            <hdist width="50%">
            </hdist>
            <coltable0 width="590">
                <rowtabarea height="100" page1="DIVLOGON" page2="DIVDATEANDTIME" textid1="logonTab" textid2="internationalisationTab">
                    <tabpage height="50" id="DIVLOGON">
                        <tr>
                            <label textid="user" width="100">
                            </label>
                            <field length="15" valueprop="user">
                            </field>
                        </tr>
                        <tr>
                            <label textid="password" width="100">
                            </label>
                            <field length="10" password="true" valueprop="password">
                            </field>
                        </tr>
                        <vdist>
                        </vdist>
                        <tr>
                            <td>
                            </td>
                            <button method="logon" textid="logon">
                            </button>
                        </tr>
                    </tabpage>
                    <tabpage display="none" height="50" id="DIVDATEANDTIME">
                        <rowarea textid="dateAndTime">
                            <tr>
                                <label textid="dateDisplay" width="100">
                                </label>
                                <field displayonly="true" length="15" valueprop="dateDisplay">
                                </field>
                                <hdist>
                                </hdist>
                                <button method="switchToMMDDYYYY" textid="setAmericanStyle">
                                </button>
                                <hdist>
                                </hdist>
                                <button method="switchToDDMMYYYY" textid="setNonAmericanStyle">
                                </button>
                            </tr>
                            <tr>
                                <label textid="timeDisplay" width="100">
                                </label>
                                <field displayonly="true" length="15" valueprop="timeDisplay">
                                </field>
                            </tr>
                        </rowarea>
                        <rowarea textid="language">
                            <tr>
                                <label textid="language" width="100">
                                </label>
                                <field length="3" valueprop="language">
                                </field>
                            </tr>
                        </rowarea>
                        <rowarea textid="styleSheet">
                            <itr>
                                <label textid="labelStyleSheet" width="100">
                                </label>
                                <field length="50" valueprop="styleSheet">
                                </field>
                            </itr>
                            <itr>
                                <label textid="wrkplacelogon.label0" width="100">
                                </label>
                                <combofix flush="server" size="1" valueprop="direction" width="120">
                                    <combooption textid="wrkplacelogon.combooption0" value="ltr">
                                    </combooption>
                                    <combooption textid="wrkplacelogon.combooption1" value="rtl">
                                    </combooption>
                                </combofix>
                            </itr>
                        </rowarea>
                        <rowarea textid="areaDecimalSeparator">
                            <itr>
                                <label textid="labelSeparator" width="100">
                                </label>
                                <radiobutton value="," valueprop="decimalSeparator">
                                </radiobutton>
                                <label asplaintext="true" textid="wrkplacelogon.label1">
                                </label>
                            </itr>
                            <itr>
                                <hdist width="100">
                                </hdist>
                                <radiobutton value="." valueprop="decimalSeparator">
                                </radiobutton>
                                <label asplaintext="true" textid="wrkplacelogon.label2">
                                </label>
                            </itr>
                        </rowarea>
                    </tabpage>
                </rowtabarea>
            </coltable0>
            <hdist width="50%">
            </hdist>
        </itr>
        <vdist height="50%">
        </vdist>
    </pagebody>
    <statusbar longtextprop="messageLongText" shorttextprop="messageShortText" typeprop="messageType" withdistance="false">
    </statusbar>
</page>
