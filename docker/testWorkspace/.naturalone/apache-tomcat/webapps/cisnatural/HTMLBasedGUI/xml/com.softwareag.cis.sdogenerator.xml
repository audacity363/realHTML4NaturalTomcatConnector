<?xml version="1.0" encoding="UTF-8"?>
<page model="com.softwareag.cis.adapter.sdo.SDOGeneratorAdapter">
    <titlebar name="SDO Java Class Generation (static binding)">
    </titlebar>
    <pagebody horizdist="false" takefullheight="true" paddingleft="20" paddingright="20" paddingtop="20" paddingbottom="20">
        <itr>
            <label name="XML Schema" width="100">
            </label>
            <field valueprop="xsdPath" width="280" flush="server" flushmethod="onXSDPathChanged">
            </field>
            <hdist width="30">
            </hdist>
            <label name="Generation Log" width="110">
            </label>
            <field valueprop="logPath" width="280">
            </field>
        </itr>
        <vdist height="10">
        </vdist>
        <itr>
            <label name="Java Package" width="100">
            </label>
            <field valueprop="javaPackage" width="280">
            </field>
            <hdist width="30">
            </hdist>
            <label name="Java Sources" width="110">
            </label>
            <field valueprop="srcPath" width="280">
            </field>
        </itr>
        <vdist height="5">
        </vdist>
        <itr>
            <hdist width="100">
            </hdist>
            <button name="Generate Java Classes" method="onGenerateJava">
            </button>
        </itr>
        <vdist height="10">
        </vdist>
        <hline height="1">
        </hline>
        <vdist height="10">
        </vdist>
        <vsplit height="100%" widthleftprop="splitLeft">
            <splitcell takefullheight="true">
                <tr>
                    <text valueprop="xsdXML" width="100%" height="100%" wrap="off">
                    </text>
                    <hdist width="10">
                    </hdist>
                </tr>               
            </splitcell>
            <splitcell takefullheight="true">
                <tr>
                    <text valueprop="generationLog" width="100%" height="100%" displayonly="true" wrap="off">
					</text>
					<hdist width="10">
					</hdist>
				</tr>
            </splitcell>
        </vsplit>
    </pagebody>
    <statusbar withdistance="false">
    </statusbar>
</page>
