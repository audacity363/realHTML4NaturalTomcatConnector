                    <button textid="refp.button46" method="onOperatorCancel" width="80">
                    </button>
                </itr>
                <vdist height="10">
                </vdist>
                <vdist height="10">
                </vdist>
            </rowarea>
        </rowdynavis>
        <rowdynavis valueprop="consatantVis">
            <rowarea foldable="false">
                <itr>
                    <label textid="refp.label32">
                    </label>
                </itr>
                <vdist height="10">
                </vdist>
                <itr>
                    <hdist width="100">
                    </hdist>
                    <label textid="refp.label33" width="100">
                    </label>
                    <field valueprop="constProp" width="150" datatype="int">
                    </field>
                    <hdist width="275">
                    </hdist>
                </itr>
                <vdist height="50">
                </vdist>
                <hline height="2" color="#D2D2D2">
                </hline>
                <vdist height="10">
                </vdist>
                <itr>
                    <hdist width="400">
                    </hdist>
                    <button textid="refp.button47" method="onConsatantOk" width="80">
                    </button>
                    <hdist width="10">
                    </hdist>
                    <button textid="refp.button48" method="onConsatantCancel" width="80">
                    </button>
                </itr>
                <vdist height="10">
                </vdist>
            </rowarea>
        </rowdynavis>
        <rowdynavis valueprop="bracketVis">
