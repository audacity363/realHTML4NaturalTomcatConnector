                <hdist width="10">
                </hdist>
                <button textid="refp.button38" method="onNewFormula" visibleprop="resetbtnvis" width="110">
                </button>
            </itr>
            <vdist height="20">
            </vdist>
            <hline height="3" color="#D2D2D2">
            </hline>
            <vdist height="10">
            </vdist>
            <itr takefullwidth="650" visibleprop="closeVisisbleProp">
                <hdist width="260">
                </hdist>
                <button textid="refp.button39" method="onOk" width="80">
                </button>
                <hdist width="10">
                </hdist>
                <button textid="refp.button40" method="onCancel" width="80">
                </button>
            </itr>
            <itr visibleprop="closeVisisbleProp">
                <icon image="images/toolbarundo.gif" method="onUndo" name="Undo">
                </icon>
                <icon image="images/toolbarredo.gif" method="onRedo" name="Redo">
                </icon>
            </itr>
        </rowarea>
        <rowdynavis valueprop="functionVis">
            <rowarea foldable="false">
                <itr>
                    <label textid="refp.label27">
                    </label>
                </itr>
                <vdist height="10">
                </vdist>
                <itr>
                    <hdist width="150">
                    </hdist>
                </itr>
                <itr>
                    <hdist width="100">
                    </hdist>
                    <label textid="refp.label28" width="100">
                    </label>
                    <field valueprop="functions" width="150" popupmethod="openIdValueCombo" popupinputonly="true">
                    </field>
                    <hdist width="275">
                    </hdist>
                </itr>
                <vdist height="50">
                </vdist>
                <hline height="2" color="#D2D2D2">
                </hline>
                <vdist height="10">
                </vdist>
                <itr>
                    <hdist width="100%">
                    </hdist>
                    <button textid="refp.button41" method="onFunctionOk" width="80">
                    </button>
                    <hdist width="10">
                    </hdist>
                    <button textid="refp.button42" method="onFunctionCancel" width="80">
                    </button>
                    <hdist width="30">
                    </hdist>
                </itr>
                <vdist height="10">
                </vdist>
            </rowarea>
        </rowdynavis>
        <rowdynavis valueprop="fieldGroupVis">
            <rowarea foldable="false">
                <itr>
                    <label textid="refp.label29">
                    </label>
                </itr>
                <itr>
                </itr>
                <rowarea foldable="false">
                    <rowtablearea2 griddataprop="lines" rowcount="10" height="200" width="610" withborder="false" hscroll="false" vscroll="auto" firstrowcolwidths="true" darkbackground="false">
                        <repeat>
                            <str valueprop="selected" showifempty="false">
                                <treenode3 width="100" withplusminus="false" singleselect="false" withlines="false">
                                </treenode3>
                            </str>
                        </repeat>
                    </rowtablearea2>
                </rowarea>
                <vdist height="20">
                </vdist>
                <itr>
                    <hdist width="350">
                    </hdist>
                    <button textid="refp.button42" method="onFieldGroupCancel" width="85">
                    </button>
                    <hdist width="10">
                    </hdist>
                </itr>
            </rowarea>
        </rowdynavis>
        <rowdynavis valueprop="operatorVis">
            <rowarea foldable="false">
                <itr>
                    <label textid="refp.label30">
                    </label>
                </itr>
                <vdist height="10">
                </vdist>
                <itr>
                    <hdist width="100">
                    </hdist>
                    <label textid="refp.label31" width="100">
                    </label>
