                    <methodlink method="onFirstLink" valueprop="firstLink">
                    </methodlink>
                </itr>
            </rowarea>
            <vdist height="20">
            </vdist>
            <itr takefullwidth="false">
                <button textid="refp.button34" method="onInsertFunction" visibleprop="funcbtnvis" width="120">
                </button>
                <hdist width="10">
                </hdist>
                <button textid="refp.button35" method="onOperator" visibleprop="operatorbtnvis" width="120">
                </button>
                <hdist width="10">
                </hdist>
                <button textid="refp.button36" method="onConsatant" visibleprop="constbtnvis" width="120">
                </button>
                <hdist width="10">
                </hdist>
                <button textid="refp.button37" method="onBracket" visibleprop="bracketbtnvis" width="110">
