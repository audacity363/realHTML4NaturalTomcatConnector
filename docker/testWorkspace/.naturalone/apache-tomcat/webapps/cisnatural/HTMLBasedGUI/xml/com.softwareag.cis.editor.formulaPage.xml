                <vdist height="10">
                </vdist>
                <itr>
                    <hdist width="400">
                        <hdist width="10">
                        </hdist>
                        <hdist width="10">
                        </hdist>
                    </hdist>
                    <button textid="refp.button49" method="onBracketOk" visibleprop="brakOkbtnvis" width="80">
                    </button>
                    <hdist width="10">
                    </hdist>
                    <button textid="refp.button50" method="onBracketCancel" width="80">
                    </button>
                </itr>
                <vdist height="10">
                </vdist>
            </rowarea>
        </rowdynavis>
    </pagebody>
    <statusbar>
    </statusbar>
</page>
