<?xml version="1.0" encoding="UTF-8"?>
<page model="com.softwareag.cis.editor.FormulaPageAdapter" translationreference="CIS_EDIT">
    <pagebody>
        <rowarea foldable="false" fixlayout="true">
            <itr>
                <label textid="refp.label25">
