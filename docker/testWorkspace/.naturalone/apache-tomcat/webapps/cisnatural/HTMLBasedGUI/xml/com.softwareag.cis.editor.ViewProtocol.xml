<?xml version="1.0" encoding="UTF-8"?>
<page model="com.softwareag.cis.editor.ViewProtocolAdapter" translationreference="CIS_TOOLS" popupwidth="700" popupheight="500">
    <pagebody vscroll="hidden" horizdist="false" takefullheight="true">
        <rowarea textid="le.prt.protocol" withleftborder="false" withrightborder="false" withbottomborder="false" withtoppadding="false">
            <itr>
                <label textid="le.prt.errors" width="100">
                </label>
                <field valueprop="numberOfErrors" length="5" displayonly="true">
                </field>
                <hdist pixelwidth="10">
                </hdist>
                <label textid="le.prt.warnings" width="100">
                </label>
                <field valueprop="numberOfWarnings" length="5" displayonly="true">
                </field>
            </itr>
            <vdist pixelheight="10">
            </vdist>
            <vdist>
            </vdist>
        </rowarea>
        <itr takefullwidth="true" height="100%" width="100%">
            <textgridsss2 griddataprop="lines" rowcount="30" width="100%" height="100%" singleselect="true" hscroll="true" directselectmethod="onLineSelected" directselectevent="onclick">
                <column textid="le.prt.id" property="id" width="40" textstyleprop="style">
                </column>
                <column textid="le.prt.tag" property="tag" width="100" textstyleprop="style">
                </column>
                <column textid="le.prt.type" property="type" width="60" textstyleprop="style">
                </column>
                <column textid="le.prt.message" property="message" width="100%" textstyleprop="style">
                </column>
            </textgridsss2>
        </itr>
    </pagebody>
</page>
