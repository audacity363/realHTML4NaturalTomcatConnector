<?xml version="1.0" encoding="UTF-8"?>
<page model="com.softwareag.cis.zzz.StatusbarOutputAdapter" translationreference="statusbar" popupwidth="700px" popupheight="320px" popupfeatures="resizable: yes;status:no" darkbackground="true">
    <pagebody horizdist="false" takefullheight="true" paddingleft="5" paddingright="5" paddingtop="5" paddingbottom="5">
        <itr takefullwidth="true">
            <textgridsss2 griddataprop="lines" rowcount="4" width="100%" selectprop="selected" singleselect="true" hscroll="true" onclickmethod="showMessage">
                <column textid="statusout.column0" property="image" width="70" align="center" imageprop="typeImage">
                </column>
                <column textid="statusout.column1" property="message" width="100%" align="left" textstyleprop="msgStyle">
                </column>
            </textgridsss2>
        </itr>
        <rowarea height="100%">
            <itr takefullwidth="true" height="100%" fixlayout="true">
                <text valueprop="detailInfo" width="100%" height="100%" displayonly="true" wrap="off" textareastyle="border: 0">
                </text>
            </itr>
        </rowarea>
        <vdist height="5">
        </vdist>
        <itr align="center">
            <button textid="statusout.button0" method="closePage">
            </button>
        </itr>
    </pagebody>
</page>
