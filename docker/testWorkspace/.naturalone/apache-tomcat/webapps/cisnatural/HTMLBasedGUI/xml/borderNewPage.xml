<?xml version="1.0" encoding="UTF-8"?>
<page model="com.softwareag.cis.editor.BorderNewPageAdapter" popupheight="250" popupwidth="650" translationreference="editor">
    <pagebody pagebodystyle="background-color: #D2D2D2">
        <vdist height="10">
        </vdist>
        <itr>
            <hdist width="400">
            </hdist>
            <button method="onOk" textid="rbnp.button2" width="60">
