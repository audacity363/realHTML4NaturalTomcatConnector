                <hdist width="10">
                </hdist>
                <icon colstyle="background-color: #9D9D9D" displaymenuindicator="true" image="../HTMLBasedGUI/images/borderWidth.gif" method="onBorderWidth" textid="rbnp.icon17" titletextid="rbnp.icon18">
                </icon>
                <hdist width="10">
                </hdist>
                <icon colstyle="background-color: #9D9D9D" displaymenuindicator="true" image="../HTMLBasedGUI/images/borderColor.gif" method="onBorderColor" textid="rbnp.icon19" titletextid="rbnp.icon20">
                </icon>
                <vdist height="10">
                </vdist>
            </itr>
            <itr>
                <hdist width="20">
                </hdist>
                <coldynavis style="border:solid;border-width:1px" takefullheight="50" valueprop="borderTypes" width="50">
                    <itr>
                    </itr>
                    <vdist height="10">
                    </vdist>
                    <itr>
                        <hdist width="5">
                        </hdist>
                        <icon image="../HTMLBasedGUI/images/border.gif" method="onBorderAll">
                        </icon>
                        <hdist width="5">
                        </hdist>
                        <icon image="../HTMLBasedGUI/images/borderLeft.gif" method="onBorderLeft">
                        </icon>
                        <hdist width="5">
                        </hdist>
                        <icon image="../HTMLBasedGUI/images/borderRight.gif" method="onBorderRight">
                        </icon>
                        <hdist width="5">
                        </hdist>
                        <icon image="../HTMLBasedGUI/images/borderTop.gif" method="onBorderTop">
                        </icon>
                        <hdist width="5">
                        </hdist>
                        <icon image="../HTMLBasedGUI/images/borderBottom.gif" method="onBorderBottom">
                        </icon>
                        <hdist width="5">
                        </hdist>
                    </itr>
                    <vdist>
                    </vdist>
                    <itr>
                        <hdist width="15">
                        </hdist>
                        <field fieldstyle="background-color: #F0F1F2" flush="server" noborder="true" valueprop="selBorderType" width="50">
                        </field>
                    </itr>
                    <vdist height="3">
                    </vdist>
                </coldynavis>
                <hdist width="130">
                </hdist>
                <coldynavis style="border:solid;border-width:1px" takefullheight="true" valueprop="borderStyle" width="120">
                    <itr>
                        <label textid="rbnp.label3">
                        </label>
                        <field flush="server" popupmethod="openIdValueCombo" valueprop="selBorderStyle" width="80">
                        </field>
                    </itr>
                </coldynavis>
                <hdist width="130">
                </hdist>
                <coldynavis style="border:solid;border-width:1px" takefullheight="true" valueprop="borderWidth" width="100">
                    <itr>
                        <label textid="rbnp.label4">
                        </label>
                        <field flush="server" popupmethod="openIdValueCombo" valueprop="selBorderWidth" width="80">
                        </field>
                    </itr>
                </coldynavis>
                <hdist width="60">
                </hdist>
                <coldynavis takefullheight="true" valueprop="borderColor" width="100">
                    <itr>
                        <label textid="rbnp.label5" width="100">
                        </label>
                        <field flush="server" valueprop="selBorderColor">
                        </field>
                    </itr>
                </coldynavis>
                <vdist>
                </vdist>
