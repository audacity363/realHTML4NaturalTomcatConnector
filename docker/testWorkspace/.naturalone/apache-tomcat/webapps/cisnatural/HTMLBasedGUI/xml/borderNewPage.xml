            <hdist width="10">
            </hdist>
            <button method="onCancel" textid="rbnp.button3" width="60">
            </button>
        </itr>
        <vdist height="10">
        </vdist>
        <rowarea areastyle="background-color: #FFFFFF">
            <vdist height="15">
            </vdist>
            <itr itrstyle="null;border:solid;border-style:double;border-width:16;border-color:#FF0000">
                <hdist width="20">
                </hdist>
                <icon colstyle="background-color: #9D9D9D" displaymenuindicator="true" image="../HTMLBasedGUI/images/border.gif" method="onBorder" nameposition="aside" textid="rbnp.icon12" titletextid="rbnp.icon14">
