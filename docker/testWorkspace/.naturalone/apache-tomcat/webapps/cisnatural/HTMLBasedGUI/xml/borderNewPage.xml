                <hdist width="10">
                </hdist>
                <icon colstyle="background-color: #9D9D9D" displaymenuindicator="true" image="../HTMLBasedGUI/images/borderStyle.gif" method="onBorderStyle" textid="rbnp.icon15" titletextid="rbnp.icon16">
