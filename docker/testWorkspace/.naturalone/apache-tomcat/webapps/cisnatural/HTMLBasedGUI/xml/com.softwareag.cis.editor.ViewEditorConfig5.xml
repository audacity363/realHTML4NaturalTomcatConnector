<?xml version="1.0" encoding="UTF-8"?>
<page model="com.softwareag.cis.editor.ViewEditorConfig5Adapter" pagename="CIS_EDIT">
    <header>
        <button textid="configSave" method="onOK" image="../HTMLBasedGUI/images/save.gif">
        </button>
    </header>
    <pagebody horizdist="false" takefullwidth="true">
        <itr height="22" fixlayout="true" takefullwidth="true">
            <label textid="configConfigFile" width="100%" asheadline="true" labelstyle="padding-left: 2px; font-size: 10px; font-weight: bold">
            </label>
        </itr>
        <vdist height="5">
        </vdist>
        <itr takefullwidth="true">
            <hdist width="5">
            </hdist>
            <coltable0 width="100%">
                <rowarea textid="configPathInfo" withtoppadding="false">
                    <itr>
                        <label textid="configName" width="100">
                        </label>
                        <textout valueprop="fileName" width="400">
                        </textout>
                    </itr>
                    <itr>
                        <label textid="configDir" width="100">
                        </label>
                        <text valueprop="project" width="432" displayonly="true" wrap="soft" rows="3">
                        </text>
                    </itr>
                </rowarea>
                <vdist height="5">
                </vdist>
            </coltable0>
            <hdist width="5">
            </hdist>
        </itr>
        <rowdynavis valueprop="showCodeGenSettings">
            <itr height="22" fixlayout="true" takefullwidth="true">
                <label textid="configCodeAssSettings" width="100%" asheadline="true" labelstyle="padding-left: 2px; font-size: 10px; font-weight: bold">
                </label>
            </itr>
            <vdist height="5">
            </vdist>
            <itr takefullwidth="true">
                <hdist width="5">
                </hdist>
                <coltable0 width="100%">
                    <rowarea textid="ec.general" withtoppadding="false">
                        <itr>
                            <label textid="configCodeDir" width="150">
                            </label>
                            <field valueprop="sourceDir" width="380">
                            </field>
                        </itr>
                        <itr>
                            <label textid="configSpacesPerTab" width="150">
                            </label>
                            <field valueprop="tabWidth" length="2" maxlength="1" datatype="int">
                            </field>
                        </itr>
                        <itr>
                            <label textid="configOneLine">
                            </label>
                            <hdist>
                            </hdist>
                            <checkbox valueprop="oneLine">
                            </checkbox>
                        </itr>
                    </rowarea>
                    <vdist height="5">
                    </vdist>
                    <rowarea textid="ec.classMembers" withtoppadding="false">
                        <itr>
                            <label textid="configPrefix" width="150">
                            </label>
                            <field valueprop="prefix" length="12" maxlength="9">
                            </field>
                        </itr>
                        <itr>
                            <label textid="memberQualifier" width="150">
                            </label>
                            <field valueprop="memberQualifier" popupmethod="openIdValueCombo" popupinputonly="true">
                            </field>
                        </itr>
                    </rowarea>
                    <vdist height="5">
                    </vdist>
                    <rowarea textid="ec.validation" withtoppadding="false">
                        <itr>
                            <label textid="disablePropNameValidation" width="150">
                            </label>
                            <hdist>
                            </hdist>
                            <checkbox valueprop="disablePropNameValidation" flush="server">
                            </checkbox>
                        </itr>
                        <itr takefullwidth ="true">
                            <label textid="propNameValidation" width="150">
                            </label>
                            <field valueprop="propNameValidation" width="100%" >
                            </field>
                        </itr>
                        <vdist height="100%">
                        </vdist>
                    </rowarea>
                    <vdist height="5">
                    </vdist>
                </coltable0>
                <hdist width="5">
                </hdist>
            </itr>
            <vdist height="100%">
            </vdist>
        </rowdynavis>
        <rowdynavis valueprop="showXMLDirectory">
            <itr height="3" itrstyle="background-color: #FFFFFF" takefullwidth="true">
            </itr>
            <itr takefullwidth="true">
                <hdist width="4">
                </hdist>
                <colarea textid="configXMLDirectorySettings" width="100%">
                    <itr>
                        <label textid="configXMLDir" width="150">
                        </label>
                        <field valueprop="xmlDir" width="380">
                        </field>
                    </itr>
                </colarea>
            </itr>
        </rowdynavis>
        <rowdynavis valueprop="showSourceDirectory">
            <itr itrstyle="background-color: #FFFFFF" takefullwidth="true">
            </itr>
            <rowarea textid="ec.java">
                <itr>
                    <label textid="configCodeDir" width="150">
                    </label>
                    <field valueprop="sourceDir" width="380">
                    </field>
                </itr>
                <vdist>
                </vdist>
            </rowarea>
        </rowdynavis>
        <rowdynavis valueprop="showCodeGenSettings">
            <itr height="22" fixlayout="true" takefullwidth="true">
                <label textid="ec.attributes" width="100%" asheadline="true" labelstyle="padding-left: 2px; font-size: 10px; font-weight: bold">
                </label>
            </itr>
            <vdist height="5">
            </vdist>
            <itr takefullwidth ="true">
                <hdist width="5">
                </hdist>
                <coltable0 width="100%">
                    <rowarea textid="ec.dynvalidvalues" withtoppadding="false">
                        <itr>
                            <label textid="ec.javaClassName" width="150">
                            </label>
                            <field valueprop="validValuesProvider" width="380">
                            </field>
                        </itr>
                    </rowarea>
                    <vdist height="5">
                    </vdist>
                </coltable0>
                <hdist width="5">
                </hdist>
            </itr>
            <vdist height="100%">
            </vdist>
        </rowdynavis>
        <rowdynavis valueprop="showCodeGenSettings">
            <itr height="22" fixlayout="true" takefullwidth="true">
                <label textid="ec.previewSettings" width="100%" asheadline="true" labelstyle="padding-left: 2px; font-size: 10px; font-weight: bold">
                </label>
            </itr>
            <vdist height="5">
            </vdist>
            <itr takefullwidth="true">
                <hdist width="5">
                </hdist>
                <coltable0 width="100%">
                    <rowarea textid="ec.previewWithLatestClasses" withtoppadding="false">
                        <itr>
                            <radiobutton valueprop="showQuickPreview" value="true" textid="ec.previewWithLatestClassesShow" hdistpixelwidth="5">
                            </radiobutton>
                        </itr>
                        <itr>
                            <radiobutton valueprop="showQuickPreview" value="false" textid="ec.previewWithLatestClassesHide" hdistpixelwidth="5">
                            </radiobutton>
                        </itr>
                    </rowarea>
                    <vdist height="5">
                    </vdist>
                </coltable0>
                <hdist width="5">
                </hdist>
            </itr>
            <itr takefullwidth="true">
                <hdist width="5">
                </hdist>
                <coltable0 width="100%">
                    <rowarea textid="ec.previewScreenTestOnly" withtoppadding="false">
                        <itr>
                            <radiobutton valueprop="previewWithDummyAdapter" value="true" textid="ec.previewWithDummyAdapter" hdistpixelwidth="5">
                            </radiobutton>
                        </itr>
                        <itr>
                            <radiobutton valueprop="previewWithDummyAdapter" value="false" textid="ec.previewWithRealAdapter" hdistpixelwidth="5">
                            </radiobutton>
                        </itr>
                    </rowarea>
                    <vdist height="5">
                    </vdist>
                </coltable0>
                <hdist width="5">
                </hdist>
            </itr>
            <vdist height="100%">
            </vdist>
        </rowdynavis>
        <vdist height="100%">
        </vdist>
    </pagebody>
    <statusbar withdistance="false">
    </statusbar>
</page>
