<?xml version="1.0" encoding="UTF-8"?>
<page model="com.softwareag.cis.admin.EditorExtensionAdapter" translationreference="CIS_TOOLS" popupwidth="700" popupheight="170">
    <pagebody horizdist="false" takefullheight="true">
        <rowarea textid="exnf.rowarea.filename" height="100%" withleftborder="false" withrightborder="false" withbottomborder="false" withtoppadding="false">
            <itr>
            </itr>
            <itr>
                <label textid="label.name" width="70">
                </label>
                <label textid="label.editor" width="50" asplaintext="true">
                </label>
                <field valueprop="newfileName" width="200" length="8">
                </field>
                <label textid="label.xml" width="30" asplaintext="true">
                </label>
                <hdist width="30">
                </hdist>
            </itr>
            <itr>
                <label name="Directory" width="70">
                </label>
                <field valueprop="newfileDirectory" width="100%" popupmethod="openIdValueCombo" popupinputonly="true">
                </field>
                <hdist width="30">
                </hdist>
            </itr>
            <vdist height="5">
            </vdist>
            <itr width="100%">
                <hdist width="50%">
                </hdist>
                <button textid="btn.ok" method="onFinishCreateNewFile">
                </button>
                <hdist>
                </hdist>
                <button textid="btn.cancel" method="closePage">
                </button>
                <hdist width="50%">
                </hdist>
            </itr>
            <vdist height="100%">
            </vdist>
        </rowarea>
    </pagebody>
    <statusbar>
    </statusbar>
</page>
