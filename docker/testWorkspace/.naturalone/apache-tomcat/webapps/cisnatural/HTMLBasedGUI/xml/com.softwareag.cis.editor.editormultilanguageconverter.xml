<?xml version="1.0" encoding="UTF-8"?>
<page model="com.softwareag.cis.multilanguage.filebased.CSVToXMLConverterAdapter">
    <titlebar name="CSVToXML Converter">
    </titlebar>
    <pagebody paddingleft="10" paddingright="10" paddingtop="10" paddingbottom="10">
        <rowarea name="Export Settings">
            <vdist height="5">
            </vdist>
            <itr>
                <label name="Select the &apos;messages chapter&apos;" width="220">
                </label>
                <combodyn2 valueprop="titleString" validvaluesprop="titleStringValues" width="200">
                </combodyn2>
            </itr>
            <itr>
                <label name="Specify the &apos;export filename&apos;" width="220">
                </label>
                <field valueprop="exportFile" width="200">
                </field>
            </itr>
            <itr>
                <label name="Select a .csv file" width="220">
                </label>
                <combodyn2 valueprop="selectedFile" validvaluesprop="validFiles" width="200">
                </combodyn2>
            </itr>
            <vdist height="5">
            </vdist>
            <itr>
                <button name="Create File" method="onCreateFile">
                </button>
            </itr>
            <vdist height="5">
            </vdist>
        </rowarea>
    </pagebody>
    <statusbar withdistance="false">
    </statusbar>
</page>
