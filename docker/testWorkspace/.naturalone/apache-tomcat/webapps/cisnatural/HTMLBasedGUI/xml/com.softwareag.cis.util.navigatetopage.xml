<?xml version="1.0" encoding="UTF-8"?>
<page model="com.softwareag.cis.adapter.NavigateToPageAdapter">
    <titlebar name="Navigation to a Specified Page during Page Init">
    </titlebar>
    <header withdistance="false">
        <button name="Navigate to page" method="onNavigate">
        </button>
    </header>
    <pagebody>
        <rowarea name="Description" height="100%">
            <vdist height="10">
            </vdist>
            <itr>
                <label name="The navigation destination must be passed as init parameter in the url. Example: navpage=/myproject/mypage.html" width="400">
                </label>
            </itr>
            <vdist height="10">
            </vdist>
            <itr>
                <label name="The navigation is immediately done when the page is loaded. This page will not be visible.">
                </label>
            </itr>
            <vdist height="10">
            </vdist>
        </rowarea>
        <rowarea name="Page Init Params" height="100%">
            <vdist height="10">
            </vdist>
            <itr>
                <label name="Navigate to page" width="200">
                </label>
                <field valueprop="navPage" width="200" ></field>
            </itr>
            <vdist height="10">
            </vdist>           
        </rowarea>
    </pagebody>
    <statusbar withdistance="false">
    </statusbar>
</page>
